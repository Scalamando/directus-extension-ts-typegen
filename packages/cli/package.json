    "dotenv": "^17.0.1",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "@directus-ts-typegen/shared": "workspace:",
    "@types/node": "^22.15.30",
    "@types/yargs": "^17.0.33",
