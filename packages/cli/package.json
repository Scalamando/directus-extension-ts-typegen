{
  "name": "directus-ts-typegen",
  "version": "0.1.0",
  "description": "A CLI to generate TypeScript types for your directus schema.",
  "keywords": [
    "directus",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Scalamando/directus-ts-typegen"
  },
  "license": "MIT",
  "author": "Raimund Canzler <git@rai-canzler.de>",
  "type": "module",
  "bin": {
    "directus-ts-typegen": "./dist/run.js"
  },
  "scripts": {
    "bin": "tsx ./src/run.ts",
    "build": "tsup"
  },
  "dependencies": {
