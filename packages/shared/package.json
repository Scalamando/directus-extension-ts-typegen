{
  "name": "@directus-ts-typegen/shared",
  "version": "0.7.0",
  "private": true,
  "description": "Core logic to generate TypeScript types for your directus schema.",
  "repository": {
    "type": "git",
    "url": "https://github.com/Scalamando/directus-extension-ts-typegen"
  },
  "license": "MIT",
  "author": "Raimund Canzler <git@rai-canzler.de>",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "dev": "tsc --watch --project tsconfig.build.json",
    "test": "vitest run",
    "test:dev": "vitest"
  },
  "dependencies": {
    "pluralize-esm": "^9.0.5"
  },
  "devDependencies": {
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.10.0"
}
